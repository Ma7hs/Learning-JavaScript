<html lang="pt-br">
    <head>
        <meta charset="UTF-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
</head>
<body>
    <script>
         //Operadores relacionais <COM ORDEM DE PRECEDÊNCIA>
            5 > 2 -> true           // maior
            7 < 4 -> false          // menor 
            8 >= 8 -> true          // maior ou igual
            9 <= 7 -> false         // menor ou igual
            5 == 5 -> true          // igual
            4 != 4 -> false         // diferente 
            
        //Operadores de identidade <SEM ORDEM DE PRECEDÊNCIA>
            5 == '5' -> true        // Tem o mesmo valor e são tipos diferentes, mas nesse caso analisa somente valor.
            5 === '5' -> false      // Não é identico pois um é do tipo String '5' o outro e do tipo Numeric.
            5 === 5 -> true         // Tem o mesmo valor e são do mesmo tipo.
            10 != '10' -> false     // Falso, pois são de tipos diferentes.
            10 !== '10' -> true     // Sim, são de tipos diferente mas o valor interno é o mesmo.
 
            
        // Operadores Lógicos -> Retorna valores Booleanos (True || False) <COM ORDEM DE PRECEDÊCIA>
            
            ! == negação -> 
            {
                !true -> false
                !false -> true
            } 
                        
            && == conjução == 'E' ->
            {
                true && true -> true
                true && false -> false
                false && true -> false
                false -> false -> false
            }
            || == disjunção == 'OU' ->
            
            {
                true || false -> true
                true || true -> true
                false || true -> true
                false || false -> false 
            }
            Exemplos ->
            {
                idade >= 15 && idade <= 17                // A idade está entre 15 e 17 anos? resposta do prompt() na web.
                estado == 'RJ' || estado == 'SP'         // O estado é RJ ou SP?
                salário > 1500 && sexo != 'M'           // Salário é maior que 1500 e sexo diferente de masculino, no caso será sexo Feminino.
            }
    </script>
    
</body>
</html>
